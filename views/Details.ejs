                padding-bottom: 1rem;
                height: max-content;
            }

            .add {
                font-size: .5rem;
            }

            th {
                font-size: .9rem;
                padding: .4rem;
            }

            .orderdata {
                font-size: .35rem;
            }

            tfoot td {
