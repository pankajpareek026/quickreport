</head>

<body onload="load()">
    <div class="mainmenu"><br>
        <br>
        <logo>
            <m style="color: orangered;  font-weight: bold; font-size: x-large; margin-left: 25px; ">Q</m>
            <m style="color:wheat ;">U</m>
            <m style="color:white ;">I</m>
            <m style="color: greenyellow;">C</m>
            <m style="color:orangered;">K</m>
            <m style="color:rgb(248, 247, 246) ;">Report</m>
        </logo>
        <span class="home"><a href="/"><i title="Quick Report Home" class="fas fa-home"></i></a></span>
        <!-- <span><a href="/dash-bord"><i title="PortFolio" class="fas fa-chart-line"></i></a></span> -->
        <span><a href="/dash-bord"><i title="DashBord" class="fas fa-chart-pie"></i></a></span>
        <span><a href="/News"><i class="fas fa-newspaper"></i></a></span>
        <span><a href="/register"><i title="User" class="far fa-user"></i></a></span>
        <!-- <span  ><a class="logout" title="Logout" href="/logout">Logout</a></span> -->
        <script src="https://widgets.coingecko.com/coingecko-coin-price-marquee-widget.js"></script>
        <!-- <coingecko-coin-price-marquee-widget  coin-ids="bitcoin,eos,ethereum,litecoin,ripple,biconomy,dog" currency="inr" background-color="#ffffff" locale="en"></coingecko-coin-price-marquee-widget> -->
    </div>
    <div class="coindetails">
        <p style="color: rgb(0, 191, 255); padding: 20px;">
            <%-CmcName-%>
                <br>
                <%-CmcSymbol-%>
        </p>
        <img src="<%-ImageSrc-%>" alt="" class="coin-logo">

        <div class="current-price">
            $ <%-parseFloat(CurrenPrice).toFixed(3)-%>
        </div>
        <br>
        <span class="change24">
            <%if(ChangeIn24H<0) {%>
                <div class="red">
                    <%-parseFloat(ChangeIn24H).toFixed(2)-%> %
                </div>
                <%}else{%>
                    <div class="green">
                        <%-parseFloat(ChangeIn24H).toFixed(2)-%> %
                    </div>
                    <%}%>

        </span>
        <div style="display: flex;">

            <span>
                <button class="change">7 D</button>
                <%if(ChangeIn7D<0) {%>
                    <p class="redc">
                        <%-parseFloat(ChangeIn7D).toFixed(2)-%>%
                    </p>
                    <%}else{%>
                        <p class="greenc">
                            <%-parseFloat(ChangeIn7D).toFixed(2)-%> %
                        </p>
                        <%}%>
            </span>

            <span>
                <button class="change">30 D</button>
                <%if(ChangeIn30D<0) {%>
                    <p class="redc">
                        <%-parseFloat(ChangeIn30D).toFixed(2)-%> %
                    </p>
                    <%}else{%>
                        <p class="greenc">
                            <%-parseFloat(ChangeIn30D).toFixed(2)-%> %
                        </p>
                        <%}%>
            </span>

            <span>
                <button class="change">60 D</button>
                <%if(ChangeIn60D<0) {%>
                    <p class="redc">
                        <%-parseFloat(ChangeIn60D).toFixed(2)-%> %
                    </p>
                    <%}else{%>
                        <p class="greenc">
                            <%-parseFloat(ChangeIn60D).toFixed(2)-%> %
                        </p>
                        <%}%>
            </span><span>
                <button class="change">90D</button>
                <%if(ChangeIn90D<0) {%>
                    <p class="redc">
                        <%-parseFloat(ChangeIn90D).toFixed(2)-%> %
                    </p>
                    <%}else{%>
                        <p class="greenc">
                            <%-parseFloat(ChangeIn90D).toFixed(2)-%> %
                        </p>
                        <%}%>
            </span>
        </div>
    </div>
    <div class="detail">
        <div class="value">
            Invested <br>
            $ <%-parseFloat(SumInvested).toFixed(3)-%>

        </div>
        <div class="value">
            <!-- <span style="font-size: small;" class=""> -->
            Current value <br>
            $ <%-parseFloat(holding*CurrenPrice).toFixed(2)-%>
                <!-- </span> -->

        </div>
        <div class="value">
            A. price <br> $ <%-parseFloat(Avp).toFixed(3)-%>

        </div>
        <div class="value">
            Holding <br>
            <%- parseFloat(holding).toFixed(3)-%>
                <%-CmcSymbol-%>

        </div>
        <div class="value">
            Volume <br>
            <%- parseFloat(volume).toFixed(3)-%>
                <%-CmcSymbol-%>
                    <!-- <span style="font-size: small;">Volume : <%- parseFloat(volume).toFixed(3)-%> <%-CmcSymbol-%> -->

        </div>

        <div class="value" style="margin-right:10px ;">
            <% if(TotalPNLP>0){%>

                PNL:
                $ <%-parseFloat((TotalPNLP*SumInvested) /100).toFixed(2)-%>
                    <p class="pnlG">(<%-parseFloat(TotalPNLP).toFixed(2)-%> % )</p>

                    <%}else{%>
                        PNL:
                        $ <%-parseFloat(TotalPNLP*SumInvested).toFixed(2) /100-%>
                            <p class="pnlR">(<%-parseFloat(TotalPNLP).toFixed(2)-%> % )</p>
                            <%}%>


                                </span>
        </div>
    </div>
    <div class="tds" style="color:rgb(253, 253, 4);">
        <button class="add" onclick="ShowOptions()">+ add Transaction
        </button>
    </div>

    <div class="tablec">
        <table border="none" id=" Ttable" style="width: 100%;">
            <tr>

                <th style="border-radius: 10px 0px 0px 0px;">ORDER TYPE</th>
                <th>PRICE</th>
                <th>UNITS</th>
                <th>COST</th>
                <th>SELL COST</th>

                <th style="border-radius: 0px 10px 0px 0px;">ACTION</th>
            <tr>
                <% var totalUNITS=0,TotalCOST=0;%>
                    <% if( results.length> 0 ){ %>
                        <% for(var i=0 ;i < results.length;i++){ totalUNITS+=parseFloat(results[i].UNITS);
                            TotalCOST+=parseFloat(results[i].TOTAL_COST )%>
            <tr>
                <form id="deleteForm" method="post">

                    <td class="orderdata">
                        <%- results[i].ORDER_TYPE.toUpperCase () -%>
                    </td>

                    <td class="orderdata">
                        <%-results[i].PRICE -%>
                    </td>

                    <td class="orderdata">
                        <%- results[i].UNITS -%>
                    </td>

                    <td class="orderdata">
                        <%- results[i].TOTAL_COST -%>
                    </td>

                    <td class="orderdata">
                        <%- results[i].SELL_COST -%>
                    </td>
